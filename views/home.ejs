<%- include('nav');-%>
  <%- include('navbar');-%>

    <div class='Home'>

      <div class="contentHome">
        <h2 data-aos="fade-up" class='home_title font-semibold' title='La Femme Originals'>
          <%=title%>
        </h2>
        <h4 data-aos="fade-up" class="font-semibold">
          <%=subtitle%>
        </h4>
        <button data-aos="fade-up" class="button-57" role="button"><span class="text">Explore</span><span>Lets Go</span></button>
      </div>
    </div>

    <section class="products" id="products">
      <div class="patterns ">
        <svg width="100%" height="100%">
          <defs>
            <pattern id="polka-dots" x="0" y="0" width="100" height="100" patternUnits="userSpaceOnUse">
              <circle fill="#be9ddf" cx="25" cy="25" r="3"></circle>
            </pattern>
            <style>
              @import url("https://fonts.googleapis.com/css?  family=Lora:400,400i,700,700i");
            </style>

          </defs>
          <text x="50%" y="60%" text-anchor="middle">
            Products
          </text>
        </svg>
      </div>
      <style>
        .btnss {
          display: flex;
          gap: 20px;
          align-items: center;
          justify-content: center;
        }
      </style>
      <div class="btnss">
        <button data-aos="fade-up" class="button-57" role="button"><a href="/under999"><span class="text"> <b> Under
                999</b></span><span>Go</span></button></a>
        <button data-aos="fade-down" class="button-57" role="button"><a href="/products"><span class="text"><b>All
                Products</b></span><span>Go</span></button></a>
        <button data-aos="fade-up" class="button-57" role="button"><a href="/under499"><span class="text"><b>Under
                499</b></span><span>Go</span></button></a>
      </div>
      <div class="productss">
        <% productss.forEach(function(product) { %>
          <div class="product-box">
            <div class="container">
              <img src="<%= product['proimg']%>" alt="cookies" class="hero-image" />



              <div class="information">
                <div class="price">INR <div>
                    <%= product['proprice']%>
                  </div>
                </div>
                <div class="name">
                  <%= product['proname']%>
                </div>

                <!-- <div class="store">
                  <%= product['prodesc']%>
                </div> -->

                <!-- <a href="#" class="button">Purchase Product</a> -->
                <a href="Products/<%= product['proid']%>"><button class="button-57" role="button"><span
                      class="text">Explore</span><span>Lets Go</span></button></a>
                <h2>&#8595;</h2>

              </div> <!-- end information -->
            </div> <!-- end container -->
          </div> <!-- end product-box -->
          <%});%>
    </section>
    <%- include('category');-%>

      <style>
        @import url(https://fonts.googleapis.com/css?family=Raleway:400,500,600);




        .product-box {
          font: 13px/23px "Raleway", Arial, sans-serif;
          color: #303336;
          padding: 40px 0;

        }



        .container {
          margin: 0 auto;
          width: 300px;
          height: fit-content;
          background: white;
          border-radius: 3px;
          position: relative
        }

        .price {
          color: rgb(0, 0, 0);
          text-align: center;

          padding: 19px 33px;
          font-size: 22px;


        }

        .information {
          text-align: center;
          margin-top: 18px;
        }

        .name {
          font-size: 22px;
        }

        .store {
          font-size: 16px;
          color: #8c98a8;
          margin-bottom: 28px;
        }

        .button {
          text-decoration: none;
          background: #49B956;
          color: white;
          font-size: 16px;
          font-weight: 500;
          padding: 12px 54px;
          border-radius: 5px;

        }
      </style>
      <div class="services">
        <div class="patterns ">
          <svg width="100%" height="100%">
            <defs>
              <pattern id="polka-dots" x="0" y="0" width="100" height="100" patternUnits="userSpaceOnUse">
                <circle fill="#be9ddf" cx="25" cy="25" r="3"></circle>
              </pattern>
              <style>
                @import url("https://fonts.googleapis.com/css?  family=Lora:400,400i,700,700i");
              </style>

            </defs>
            <text x="50%" y="60%" text-anchor="middle">
              Services
            </text>
          </svg>
        </div>
        <div class="boxes">
          <% Scontent.forEach(function(eachh) { %>
            <div class="box"
              style="background: url('<%= eachh[`Simg`] %>'); background-position: center; background-size: cover;">
              <div class="upper">
                <h1>
                  <%= eachh['Stitle'] %>
                </h1>
                <p>
                  <%= eachh['Sdesc'] %>
                </p>
              </div>
              <div class="lower"><img src="{{content[3]}}" alt=""></div>
            </div>
            <% }); %>
        </div>

      </div>

      <!-- <br><br><br><br><br><br><br><br><br><br> -->
      <%- include('footer');-%>