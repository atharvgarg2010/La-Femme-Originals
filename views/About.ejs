<%- include('nav');-%>
<!--  -->
<%- include('navbar', {a:'d'});-%>
<%- include('category');-%>

<div class="services">
  <div class="patterns ">
    <svg width="100%" height="100%">
      <defs>
        <pattern id="polka-dots" x="0" y="0" width="100" height="100" patternUnits="userSpaceOnUse">
          <circle fill="#be9ddf" cx="25" cy="25" r="3"></circle>
        </pattern>
        <style>
          @import url("https://fonts.googleapis.com/css?  family=Lora:400,400i,700,700i");
        </style>

      </defs>
      <text x="50%" y="60%" text-anchor="middle">
        Services
      </text>
    </svg>
  </div>
  <div class="boxes">
      <% Scontent.forEach(function(eachh) { %>
      <div class="box" style="background: url('<%= eachh[`Simg`] %>'); background-position: center; background-size: cover;">
        <div class="upper">
          <h1>
            <%= eachh['Stitle'] %>
          </h1>
          <p><%= eachh['Sdesc'] %></p>
        </div>
        <div class="lower"><img src="{{content[3]}}" alt=""></div>
      </div>
      <% }); %>
    </div>

</div>